[user]
	name = Felix Yadomi
	email = dev.yadomi@gmail.com
[core]
	excludesfile = /Users/yadomi/.gitignore_global
	editor = subl -w -n
	pager = less
[alias]
	lg = log --abbrev-commit --pretty=tformat:'%Cred%h%Creset %C(yellow)%an%Creset %C(green)%ad%Creset %s %C(auto)%d%Creset%C(bold blue) (%ar) %Creset' --date=short
	ignore = "!gi() { curl -L -s https://www.gitignore.io/api/$@ ;}; gi"
	comit='commit'
	comi='commit'
	chekcout='checkout'
	chekout='checkout'
	pish='push'
	posh='push'
	rest='reset'
	undo='reset --soft HEAD^'
[color]
	diff = auto
	status = auto
	branch = auto
[push]
	default = upstream
[status]
	showUntrackedFiles = all
[color "branch"]
	upstream = cyan